# ***************************************
# Backend Configuration
# ***************************************

# Backend Base URL
BACKEND_URL=http://debugiq-backend.railway.app

# Frontend Base URL (if needed on the backend for CORS or other purposes)
FRONTEND_URL=http://debugiq-frontend.railway.app

# ***************************************
# Streamlit Configuration (if applicable)
# ***************************************

# Port for Streamlit App
STREAMLIT_PORT=10000

# ***************************************
# Security & Application Keys
# ***************************************

# Secret Key for Backend (if applicable)
SECRET_KEY=your-secret-key-here

# API Key for External Services (if any)
# Replace with actual keys for services like OpenAI, Stripe, etc.
API_KEY=your-api-key-here

# ***************************************
# Application Environment
# ***************************************

# Mode for the application (development, staging, production)
APP_ENV=development

# Debug mode (True for development, False for production)
DEBUG=True
